{
    "name": "aguilarm/mika-aguilar",
    "description": "Personal website for Mika Aguilar",
    "type": "project",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Mika Aguilar",
            "email": "mika.aguilar@gmail.com"
        }
    ],
    "repositories": [
      {
        "type":"composer",
        "url":"https://packages.drupal.org/8"
      }
    ],
    "require": {
        "cweagans/composer-patches": "^1.6",
        "composer/installers": "^1.6",
        "drupal-composer/drupal-scaffold": "^2.5",
        "drupal/core": "^8.6",
        "drush/drush": "^9.5",
        "drupal/health_check": "^1.0",
        "drupal/redis": "^1.1",
        "drupal/twig_tweak": "^2.1",
        "drupal/adminimal_theme": "^1.3",
        "drupal/adminimal_admin_toolbar": "^1.8",
        "drupal/pathauto": "^1.3",
        "drupal/metatag": "^1.7"
    },
    "scripts": {
      "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold"
		},
		"extra" : {
			"installer-paths": {
				"public/core": ["type:drupal-core"],
				"public/libraries/{$name}": ["type:drupal-library"],
				"public/modules/contrib/{$name}": ["type:drupal-module"],
				"public/themes/contrib/{$name}": ["type:drupal-theme"]
			}
		}
}
